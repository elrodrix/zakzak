{
  "name": "zakzak",
  "version": "0.1.0",
  "description": "Zakzak is a unit-test-like benchmarking framework for Node.js",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "prepack": "npm run clean && npm run compile:src",
    "clean": "rimraf dist",
    "compile": "npm run compile:src",
    "compile:src": "tsc --project ./src/tsconfig.json",
    "compile:test": "tsc --project ./test/tsconfig.json",
    "lint": "eslint . --ext .ts",
		"build": "npm run clean && npm run compile && npm run lint",
		"build:test": "npm run clean && npm run compile:test && npm run lint",
    "pretest": "npm run build:test",
    "test": "mocha -r source-map-support/register ./dist/test/**/*.test.js",
    "install-cli": "npm run build && npm install -g",
    "start": "node dist/src/cli-script.js"
  },
  "repository": "Dynatrace/zakzak",
  "keywords": [
    "nodejs",
    "benchmarking",
    "microbenchmarking",
    "unit-test",
    "zakzak"
  ],
  "bin": {
    "zakzak": "dist/src/cli-script.js"
  },
  "author": "Lukas Kurz",
  "license": "Apache-2.0",
  "files": [
    "dist/src"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/figlet": "^1.2.0",
    "@types/lodash": "4.14.116",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/sinon": "^7.0.13",
    "@types/table": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sinon": "^7.4.2",
    "source-map-support": "^0.5.13",
    "tscpaths": "0.0.9",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.0",
    "csv-writer": "^1.5.0",
    "es6-promise-pool": "^2.5.0",
    "figlet": "^1.2.3",
    "globby": "^10.0.1",
    "lodash": "^4.17.15",
    "table": "^5.4.6",
    "xml-js": "^1.6.11"
  }
}
