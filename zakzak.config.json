{
    "benchmark": {
        "warmup": {
			"enable": true,
			"minTime": 50000000,
			"maxTime": 5000000000,
            "allowJIT": true
        },
        "measure": {
            "saveTimes": true
        },
        "overhead": {
            "enable": true
		},
		"maxTries": 3
    },
    "manager": {
        "runParallel": false,
        "printTree": true
    },
    "cli": {
        "verbose": false,
        "quiet": false,
        "pattern": "*.bench.js",
		"path": "./",
		"exporter": ""
    }
}