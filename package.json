{
  "name": "zakzak",
  "version": "0.1.0",
  "description": "Zakzak is a unit-test-like benchmarking framework for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepack": "npm run clean && npm run compile:src",
    "clean": "rimraf dist",
    "compile": "npm run compile:src",
    "compile:src": "tsc -p .",
    "lint": "tslint --project ./tsconfig.json --format verbose",
    "build": "npm run clean && npm run compile && npm run lint",
    "install-cli": "npm run build && npm install -g",
    "start": "node dist/cli-script.js"
  },
  "repository": "Dynatrace/zakzak",
  "keywords": [
    "nodejs",
    "benchmarking",
    "microbenchmarking",
    "unit-test",
    "zakzak"
  ],
  "bin": {
    "zakzak": "dist/cli-script.js"
  },
  "author": "Lukas Kurz",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/lodash": "4.14.116",
    "@types/node": "^12.7.1",
    "@types/table": "^4.0.7",
    "rimraf": "^3.0.0",
    "tscpaths": "0.0.9",
    "tslint": "^5.20.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.0",
    "csv-writer": "^1.5.0",
    "figlet": "^1.2.3",
    "globby": "^10.0.1",
    "lodash": "^4.17.15",
    "table": "^5.4.6",
    "xml-js": "^1.6.11"
  }
}
