{
  "name": "zakzak",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run build && npm run start-with-native",
    "start-with-native": "node --allow-natives-syntax dist/index.js",
    "build": "npm run build-ts && npm run translate-paths && npm run prepend-shebang",
    "build-debug": "npm run build-ts && npm run translate-paths",
    "build-ts": "tsc --project tsconfig.json",
    "translate-paths": "tscpaths -p tsconfig.json -s ./src -o ./dist",
    "prepend-shebang": "./shebang.sh",
    "install-cli": "npm run build && npm install -g"
  },
  "bin": {
    "zakzak": "dist/index.js"
  },
  "author": "Lukas Kurz",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cli-table": "^0.3.0",
    "@types/figlet": "^1.2.0",
    "@types/globby": "^9.1.0",
    "@types/lodash": "4.14.116",
    "@types/node": "^12.7.1",
    "tscpaths": "0.0.9",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "commander": "^3.0.0",
    "csv-writer": "^1.5.0",
    "figlet": "^1.2.3",
    "globby": "^10.0.1",
    "lodash": "^4.17.15",
    "v8-natives": "^1.1.0",
    "xml-js": "^1.6.11"
  }
}
