{
    "benchmark": {
        "warmup": {
            "enable": true,
			"maxTime": 500000000,
			"increaseFactor": 2,
            "allowJIT": true
        },
        "measure": {
            "cycles": 100,
            "saveTimes": true
        },
        "overhead": {
            "enable": true
        }
    },
    "manager": {
        "runParallel": false,
        "printTree": true
    },
    "cli": {
        "verbose": false,
        "quiet": false,
        "pattern": "*.bench.js",
        "path": "./"
    }
}